{
  "name": "vidgenie-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx",
    "code-quality": "npm run type-check && npm run lint:check",
    "fix-all": "npm run lint:fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx scripts/seed.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:seed": "supabase db reset --db-url postgresql://postgres:postgres@localhost:54322/postgres",
    "supabase:types": "supabase gen types typescript --local > src/lib/supabase/types.ts",
    "supabase:studio": "supabase studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:oauth": "jest tests/auth/google-oauth.test.ts",
    "test:integration": "jest tests/integration/",
    "inngest:dev": "npx inngest-cli@latest dev",
    "type-check": "tsc --noEmit",
    "audit:unused": "npx depcheck && npx ts-prune",
    "clean:deps": "echo 'Cleanup completed - manual verification needed'",
    "audit:bundle": "npm run build && npx @next/bundle-analyzer",
    "mcp:filesystem": "node mcp/filesystem-server.js",
    "mcp:database": "node mcp/database-server.js",
    "mcp:api": "node mcp/api-integration-server.js",
    "mcp:inspector": "npx @modelcontextprotocol/inspector",
    "mcp:test": "node -e \"console.log('MCP Servers disponibles:'); console.log('- Filesystem: npm run mcp:filesystem'); console.log('- Database: npm run mcp:database'); console.log('- API Integration: npm run mcp:api'); console.log('- Inspector: npm run mcp:inspector');\""
  },
